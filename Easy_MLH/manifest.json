{
	"name": "Easy-MLH",
	"version": "1.0",
	"description": "An easy way to manage the MLH hackathon you're participating in!",
	"permissions": [
		"storage"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"page_action": {
		"default_popup": "popup.html"
	},
	"manifest_version": 2
}